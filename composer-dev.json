{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "wikimedia/composer-merge-plugin": "^2.0",
    "ext-zip": "*",
    "ext-zlib": "*",
    "ext-gd": "*",
    "ext-pdo": "*",
    "motor-cms/motor-backend": "^1.3",
    "motor-cms/motor-core": "^1.3",
    "motor-cms/motor-cms": "^1.3",
    "motor-cms/motor-media": "^1.3",
    "motor-cms/motor-revision": "^1.3",

    "partymeister/accounting": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan motor:setpackagedev false"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan motor:setpackagedev false"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "wikimedia/composer-merge-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "motor-cms/motor-cms"
      ]
    },
    "merge-plugin": {
      "include": [
        "packages/*/composer-dev.json"
      ],
      "require": [
      ],
      "recurse": true,
      "replace": false,
      "ignore-duplicates": true,
      "merge-dev": true,
      "merge-extra": true,
      "merge-extra-deep": false,
      "merge-replace": true,
      "merge-scripts": false
    }
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "laravel/pint": "^0.2.1",
    "nunomaduro/larastan": "^2.0",
    "spatie/laravel-ray": "^1.25"
  }
}
