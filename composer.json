{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "ext-gd": "*",
    "motor-cms/motor-backend": "^1.3",
    "motor-cms/motor-core": "^1.3",
    "motor-cms/motor-cms": "^1.3",
    "motor-cms/motor-media": "^1.3",
    "motor-cms/motor-revision": "^1.3",

    "partymeister/accounting": "^1.3",
    "partymeister/competitions": "^1.3",
    "partymeister/core": "^1.3",
    "partymeister/frontend": "^1.3",
    "partymeister/slides": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "laravel/pint": "^0.2.1",
    "nunomaduro/larastan": "^2.0",
    "spatie/laravel-ray": "^1.25"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi -vvvv"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "wikimedia/composer-merge-plugin": false
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "motor-cms/motor-cms"
      ]
    }
  }
}
